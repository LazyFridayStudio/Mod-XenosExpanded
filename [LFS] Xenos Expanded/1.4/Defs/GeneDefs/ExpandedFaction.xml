<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <FactionDef ParentName="PirateBandBase">
        <defName>Stanads</defName>
        <label>Stanads</label>
        <description>A Strong group of survivors have agreed to work together as a primarily beneficial deal. The group consists of Dhucoih, making about 30%, and Trugnoth, with 70%. The Dhucoih is the group's brains, whereas the Trugnoth are the soldiers. This rag-tag group is more like a band of pirates than a colony, Scavaging what ever gear they can get their hands on</description>
        <canMakeRandomly>true</canMakeRandomly>
        <requiredCountAtGameStart>1</requiredCountAtGameStart>
        <colorSpectrum Inherit="False">
            <li>(0.0, 0.1, 1)</li>
            <li>(0.88, 0.7, 1)</li>
        </colorSpectrum>
        <factionIconPath>UI/Factions/StanadsIcon</factionIconPath>
        <requiredMemes>
            <li MayRequire="Ludeon.RimWorld.Ideology">Raider</li>
        </requiredMemes>
        <xenotypeSet>
            <xenotypeChances Inherit="False">
                <Xeno_Trugnoth MayRequire="Ludeon.RimWorld.Biotech">0.7</Xeno_Trugnoth>
                <Xeno_Dhucoih MayRequire="Ludeon.RimWorld.Biotech">0.3</Xeno_Dhucoih>
            </xenotypeChances>
        </xenotypeSet>
        <disallowedPrecepts>
            <li MayRequire="Ludeon.RimWorld.Ideology">ApparelDesired_Strong_Subordinate</li> <!-- Never generate with any apparel precepts -->
            <li MayRequire="Ludeon.RimWorld.Ideology">ApparelDesired_Soft_Subordinate</li>
        </disallowedPrecepts>
        <maxConfigurableAtWorldCreation>9999</maxConfigurableAtWorldCreation>
        <configurationListOrderPriority>61</configurationListOrderPriority>
        <startingCountAtWorldCreation>1</startingCountAtWorldCreation>
        <canSiege>true</canSiege>
        <canStageAttacks>true</canStageAttacks>
    </FactionDef>
</Defs>